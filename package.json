{
  "name": "vue3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,build,config}/**/*.{js,json,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/style/**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "element-plus": "^1.2.0-beta.6",
    "js-export-excel": "^1.1.4",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "sass": "^1.45.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
